    <string name="ingredient_text1">
        Eggs (4)
        \nMilk (1 cup)
        \nSalt (1/4 tsp)
        \nButter
        \nFlour (1 cup)
        \nPowdered Suger
    </string>
    <string name="ingredient_text2">
        Sliced Carrots (2 lb)
        \nVelveeta Cheese (20 oz)
        \nButter
        \nOnion Powder
        \nSalt
        \nPepper

